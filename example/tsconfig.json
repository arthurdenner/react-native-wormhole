{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "typeRoots": ["index.d.ts"],
    "moduleResolution": "node"
  }
}
